							<td style="padding:3px 5px;border-right: 1px solid black;width: 150px; font-size:8px;" ><%= students.name %></td>
							<td style="padding:3px 5px; font-size:8px;">State Code :</td>
							<td style="padding:3px 5px;border-right: 1px solid black;width: 120px; font-size:8px;"><%= students.statecode %></td>
							<td style="padding:3px 5px; font-size:8px;">Invoice Date :</td>
							<td style="padding:3px 5px; font-size:8px;"><%= students.Date %></td>
						</tr>
						<tr>
							<td style="padding:3px 5px; font-size:8px;">&nbsp;</td>
							<td style="padding:3px 5px; border-right: 1px solid black; font-size:8px;">&nbsp;</td>
